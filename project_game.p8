-- player updates

function move_player()

	local lx=player.x
	local ly=player.y
	
	if (btn(0)) player.x-=1
	if (btn(1)) player.x+=1
	if (btn(2)) player.y-=1
	if (btn(3)) player.y+=1

	if collide(player) then
		player.x=lx
		player.y=ly
	end
end

