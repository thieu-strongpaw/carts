-- npc logic

function update_npcs()
	for npc in all(npc_list) do
		if npc.visible ~= false then 
			npc_move(npc)
		
			if abs(npc.x - player.x) < 8 and abs(npc.y - player.y) < 8 then
				npc.attacking = true
				game_state = "combat"
				current_enemy = npc
				combat_question = npc.question
				choices = {npc.answers[1], npc.answers[2], npc.answers[3], "quit"}
				correct_choice = npc.correct
				selected = 1
				feedback = "" 
				else 
					npc.attacking = false
			end			
		end
	end
end

function npc_move(npc)
		local speed = 1
		local dx, dy = 0, 0
		
		-- movement logic (same as before)
		if npc.state == "right" then
			dx = speed
			if npc.x >= npc.start_x + 32 then npc.state = "down" end
		elseif npc.state == "down" then
			dy = speed
			if npc.y >= npc.start_y + 24 then npc.state = "left" end
		elseif npc.state == "left" then
			dx = -speed
			if npc.x <= npc.start_x then npc.state = "up" end
		elseif npc.state == "up" then
			dy = -speed
			if npc.y <= npc.start_y then npc.state = "right" end
		end

		npc.x += dx
		npc.y += dy
end

