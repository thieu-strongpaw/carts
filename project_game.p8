function update_combat()
 local cols = 2
 
 if btnp(2) then selected -= cols end -- up
 if btnp(3) then selected += cols end -- down
 if btnp(0) then selected -= 1 end   -- left
 if btnp(1) then selected += 1 end   -- right

 if selected < 1 then selected = #choices end
 if selected > #choices then selected = 1 end

 if btnp(4) or btnp(5) then
 	local choice = choices[selected]

 	if choice == "quit" then
 	if current_enemy then
  	current_enemy.visible = false
  	current_enemy.attacking = false
 	end
  	game_state = "overworld"
  	current_enemy = nil
  	feedback = ""
 	else
	-- check if correct
 		if selected == correct_choice then
  		feedback = "correct!"
  		player.score += 1
  		sfx(0)
  	else
  	 feedback = "wrong!"
   	sfx(1)
  	end
 	end
	end
end

