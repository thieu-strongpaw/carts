-- draw combat menu

function draw_combat_menu()
    -- tile(4,28) to tile(15,28)
    local x0 = 4*8   -- 32
    local y0 = 13*8  -- 224
    local x1 = 15*8  -- 120
    local y1 = y0 + 16 -- enough for 2 lines of text

    -- backdrop
   -- fillp(0b0101010101010101)
   -- rectfill(x0-4, y0-8, x1+4, y1+16, 1)
   -- fillp()
   -- rect(x0-4, y0-8, x1+4, y1+16, 7)

    -- assume choices = {"yes", "no", "maybe", "ask again"}
    local cols = 2
    local rows = 2
    local cell_w = (x1 - x0) / cols
    local cell_h = 12

    for i=1,#choices do
        local row = flr((i-1) / cols)
        local col = (i-1) % cols

        local tx = x0 + col * cell_w + 8
        local ty = y0 + row * cell_h - 4

        local colr = (i == selected) and 10 or 7
        print(choices[i], tx, ty, colr)

        if i == selected then
            print(">", tx-6, ty, 10)
        end
    end
end
