function _update()
	if game_state == "overworld" then
		update_player()
		update_npcs()
	 update_items()
	elseif game_state == "combat" then
		update_combat()
	end
end


function _draw()
	cls()
	if game_state == "overworld" then
--	 camera(player.x-63, player.y-63)
		map()
		draw_npcs()
		draw_items()
		draw_player()
		print("score: "..player.score,2,2,7)
	elseif game_state == "combat" then
		draw_combat_screen()
		draw_combat_menu()
	end
	pal()
end

