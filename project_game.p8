-- tilemap collision check
function collide(o)
	local x1=flr(o.x/8)
	local y1=flr(o.y/8)
	local x2=flr((o.x+4)/8)
	local y2=flr((o.y+4)/8)
	
	local a=fget(mget(x1,y1),0)
	local b=fget(mget(x1,y2),0)
	local c=fget(mget(x2,y1),0)
	local d=fget(mget(x2,y2),0)

	return a or b or c or d 
end

