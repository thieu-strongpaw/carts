-- initializers

function init_game_state()
	level = 1
	game_state = "overworld"
end

function init_player()
	player = {
		x=32,
		y=32,
		score=0,
		inventory = {
			weapon=0,
			item=0,
			armor=0
		}
	}
end

function init_npcs()
	npc_list = {}
	for npc in all(npc_data) do
		add(npc_list, {
			sprite = npc.sprite,
			x = npc.x,
			y = npc.y,
			start_x = npc.x,
			start_y = npc.y,
			state = "right",
			attacking = false,
			drop_item = 1, 
			visible = true,
			question = npc.question,
			answers = npc.answers,
			correct = npc.correct
			})
		end
end

function init_items()
	item_list = {}
	
	for item in all(item_data) do
		add(item_list, {
			sprite = item.sprite,
			x = item.x,
			y = item.y,
			visible = item.visible
			})
	end
end

