-- draw combat enemy

-- meta sprite 
function draw_metasprite(sprites, x, y, w, flip_x, flip_y)
	for i=1, #sprites do
		local s = sprites[i]
		local col = (i-1) % w
		local row = flr((i-1) / w)
		local draw_x = x + col * 8
		local draw_y = y + row * 8 
	
		spr(s, draw_x, draw_y, 1, 1, flip_x, flip_y)
	end
end
